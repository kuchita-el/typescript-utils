{
  "name": "typescript-utils",
  "version": "1.0.0",
  "description": "TypeScript utility functions library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{md,json,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{md,json,yaml,yml}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "typescript",
    "utils",
    "utilities"
  ],
  "author": {
    "name": "Kuchita EL",
    "email": "el.kuchita@gmail.com",
    "url": "https://gihub.com/kuchita-el"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ],
    "**/*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  }
}
